{
  "name": "promise-endeavour",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/leahciMic/promise-endeavour.git"
  },
  "author": "Michael Leaney <leahcimic@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "prepare": "tsc --project ./tsconfig.esnext.json && tsc --project ./tsconfig.es5-module.json && tsc --project ./tsconfig.es5.json"
  },
  "module": "./dist-es5-module/promiseEndeavour.js",
  "main": "./dist/promiseEndeavour.cjs.js",
  "esnext": "./dist-esnext/promiseEndeavour.js",
  "types": "./dist-es5-module/promiseEndeavour.d.ts",
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.0",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.2",
    "typedoc": "^0.11.1",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "node-fetch": "^2.1.2"
  }
}
